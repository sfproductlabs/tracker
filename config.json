{
    "AccountHashMixer": "",
    "AllowOrigin": "*",
    "ApiVersion": 1,
    "Consume": [
        {
            "Note": "Not implemented",
            "Service": "http"
        },
        {
            "ByteLimit": 67108864,
            "CACert": "./.setup/keys/rootCa.crt",
            "Cert": "./.setup/keys/nats-client.crt",
            "Critical": true,
            "Filter": [
                {
                    "Alias": "log",
                    "Id": "tic.log.\u003e",
                    "Type": "chan"
                },
                {
                    "Alias": "count",
                    "Id": "tic.count.\u003e",
                    "Type": "chan"
                },
                {
                    "Alias": "update",
                    "Id": "tic.update.\u003e",
                    "Type": "chan"
                }
            ],
            "Format": "json",
            "Hosts": [
                "tls://localhost:4222"
            ],
            "Key": "./.setup/keys/nats-client.key",
            "MessageLimit": 65536,
            "Secure": true,
            "Service": "nats"
        }
    ],
    "Debug": true,
    "DefaultRedirect": "https://sfpl.io",
    "Domains": [
        "localhost",
        "dev.sfpl.io",
        "tr.sfpl.io",
        "api.sfpl.io",
        "msgxc.sfpl.io",
        "tr.valid9.com",
        "valid9.tr.sfpl.io",
        "chat.sfpl.io"
    ],
    "GeoIPVersion": 1,
    "IPv4GeoIPCSVDest": "IP2LOCATION-LITE-DB11.CSV",
    "IPv4GeoIPZip": "./.setup/geoip/IP2LOCATION-LITE-DB11.CSV.ZIP",
    "IPv6GeoIPCSVDest": "IP2LOCATION-LITE-DB11.IPV6.CSV",
    "IPv6GeoIPZip": "./.setup/geoip/IP2LOCATION-LITE-DB11.IPV6.CSV.ZIP",
    "IdleTimeoutSeconds": 20,
    "IgnoreInsecureTLS": true,
    "IgnoreProxyOptions": false,
    "IgnoreQueryParamsKey": "igqp",
    "MaxHeaderBytes": 1048576,
    "MaximumConnections": 150000,
    "Notify": [
        {
            "CACert": "./.setup/keys/rootCa.crt",
            "Cert": "./.setup/keys/cassandra-client.crt",
            "Connections": 2,
            "Context": "sfpla",
            "Critical": true,
            "Filter": [
                {
                    "Alias": "log",
                    "Id": "log",
                    "Queries": [
                        {
                            "Fields": [
                                {
                                    "Id": "name",
                                    "Type": "string"
                                },
                                {
                                    "Id": "hostname",
                                    "Type": "string"
                                },
                                {
                                    "Id": "level",
                                    "Type": "int"
                                },
                                {
                                    "Id": "msg",
                                    "Type": "string"
                                },
                                {
                                    "Id": "ip",
                                    "Type": "string"
                                },
                                {
                                    "Id": "username",
                                    "Type": "string"
                                },
                                {
                                    "Id": "ldate",
                                    "Type": "date"
                                },
                                {
                                    "Id": "ltime",
                                    "Type": "time"
                                }
                            ],
                            "QueryType": "json",
                            "Statement": "INSERT INTO logs JSON ?"
                        }
                    ]
                },
                {
                    "Alias": "count",
                    "Id": "count",
                    "Queries": [
                        {
                            "Fields": [
                                {
                                    "Id": "id",
                                    "Type": "string"
                                },
                                {
                                    "Id": "type",
                                    "Type": "string"
                                }
                            ],
                            "Statement": "UPDATE counters set total=total+1 where id=? AND type=?"
                        }
                    ]
                },
                {
                    "Alias": "update",
                    "Id": "update",
                    "Queries": [
                        {
                            "Fields": [
                                {
                                    "Default": "id",
                                    "Id": "id",
                                    "Type": "string"
                                },
                                {
                                    "Id": "updated",
                                    "Type": "datetime"
                                },
                                {
                                    "Id": "msg",
                                    "Type": "string"
                                }
                            ],
                            "Statement": "INSERT INTO updates (id, updated, msg) values (?,?,?)"
                        }
                    ]
                }
            ],
            "Hosts": [
                "127.0.0.1"
            ],
            "Key": "./.setup/keys/cassandra-client.key",
            "Prune": [
                {
                    "ClearAll": false,
                    "ClearNumericParams": true,
                    "ClearParams": true,
                    "Fields": [
                        {
                            "DestParamHash": "iphash",
                            "Id": "ip",
                            "Type": "inet"
                        }
                    ],
                    "IgnoreCFlags": [],
                    "PageSize": 10000,
                    "TTL": 2592000,
                    "Table": "events"
                }
            ],
            "Retries": 3,
            "Secure": false,
            "Service": "cassandra",
            "Timeout": 300
        },
        {
            "CACert": "./.setup/keys/rootCa.crt",
            "Cert": "./.setup/keys/nats-client.crt",
            "Context": "toc.track.notify",
            "Format": "json",
            "Hosts": [
                "tls://localhost:4222"
            ],
            "Key": "./.setup/keys/nats-client.key",
            "Secure": true,
            "Service": "nats"
        }
    ],
    "PrefixPrivateHash": "",
    "ProxyDailyLimit": 0,
    "ProxyDailyLimitChecker": "cassandra",
    "ProxyForceJson": false,
    "ProxyPort": ":8080",
    "ProxyPortRedirect": "",
    "ProxyPortTLS": ":8443",
    "ProxyUrl": "http://localhost:3030",
    "ProxyUrlFilter": ".*(dontlogthese|login|css|js|json|jpeg|jpg|gif|png|ico)",
    "PruneLimit": 100000,
    "PruneLogsSkip" : false,
    "PruneLogsOnly": false,
    "PruneLogsPageSize": 0,
    "PruneLogsTTL": 2592000,
    "PruneSkipToTimestamp": 0,
    "PruneUpdateConfig": false,
    "ReadHeaderTimeoutSeconds": 20,
    "ReadTimeoutSeconds": 20,
    "SchemaVersion": 3,
    "StaticDirectory": "public",
    "TLSCert": "./.setup/keys/nginx.crt",
    "TLSKey": "./.setup/keys/nginx.key",
    "TempDirectory": "./tmp/",
    "UrlFilter": "http(s|)://(api|tr|dev|spark|superset|jupyter|msgxc)(\\.staging\\.|\\.|)(valid9\\.com|sfpl\\.io|localhost)(:[0-9]*|)(/[^\\?#]*)(\\?|#|)",
    "UrlFilterMatchGroup": 6,
    "UseGeoIP": true,
    "UseLocalTLS": true,
    "UseRegionDescriptions": true,
    "UseRemoveIP": true,
    "WriteTimeoutSeconds": 20
}